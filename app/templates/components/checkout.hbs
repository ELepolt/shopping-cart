<div class="row text-center">
  {{#if this.cart.cartIsEmpty}}
    <div>Cart is empty</div>
  {{else}}
    <div>
      {{#each-in this.cart.displayProductsDictionary as |productTitle price|}}
        <div>
          <span>{{productTitle}}</span>
          <span>{{price}}</span>
        </div>
      {{/each-in}}
    </div>
    <div class="total-price">{{this.cart.totalCartPrice}}</div>
  {{/if}}
</div>
{{yield}}